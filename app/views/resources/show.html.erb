<!-- once a resource is created this comes up -->
<div id="resources">
  <div class="resource">
    <h2 class="title">
      <%= @resource.company %>
    </h2>
    <p class="description">
      <%= @resource.description %>
    </p>
    <p class="url">
      <%= link_to(truncate(@resource.url, :length => 140), @resource.url, target: '_blank') %>
    </p>
  </div>
</div>

<div id="links">
<!--   # btn class styles buttons -->
  <%= link_to "Back", resources_path %>

</div>
