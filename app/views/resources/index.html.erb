<!-- lists all resources -->
<div id="resources">
  <% @resources.each do |resource|  %>
    <div class="resource">
      <h2>
        <%= link_to resource.company, resource %>  <!-- can edit title -->
      </h2>
    </div>
    <p>
      <%= resource.description  %>
    </p>
    <p>
      <%= resource.url  %>
    </p>
  <% end %>
</div>
